{
  "builds": [
    {
      "src": "./index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/"
    },
    {
      "src": "/api/file(.*)",
      "dest": "/api/file$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
